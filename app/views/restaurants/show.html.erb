<h1>Restaurant</h1>
<h2><%= @restaurant.name %></h2>
<p><%= @restaurant.address %></p>
<p><%= @restaurant.phone_number %></p>
<p><%= @restaurant.category %></p>
<%= link_to  "Back Restaurant", restaurants_path, class: "btn btn-primary"%>
<%= link_to  "New Restaurant", new_restaurant_path, class: "btn btn-primary"%>
<%= link_to  "Edit Restaurant", edit_restaurant_path(@restaurant), class: "btn btn-primary"%>
<%= link_to  "New Review", restaurant_reviews_path(@restaurant), class: "btn btn-primary"%>